<div class="gb-share" data-role="main">
  {{#if showLabel}}
	   <div class="gb-share-title">分享到：</div>
  {{/if}}
  <div class="gb-share-con">
  	{{#each sharelist}}
  		<a class="gb-share-btn gb-share-btn-{{shareType}}" href="javascript:;" data-site="{{shareType}}" title="分享到{{label}}">
  			<i class="gb-share-ico-font">{{iconText}}</i>
  			<span class="gb-share-btn-tit">{{label}}</span>
  		</a>
  	{{/each}}
  </div>
  <b class="gb-share-ico-preset"></b><b class="gb-share-ico-preset1"></b>
</div>
